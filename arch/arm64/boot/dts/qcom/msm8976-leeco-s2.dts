&pm8950_gpios {
		pm_clk: cdc_pm{
		pins = "gpio1";
		function = "func1";
		output-low;
		bias-disable;
		qcom,drive-strength = <2>;
		drive-strength = <2>;
		power-source = <0>;
	};


		pm8950_gpio_7: wcd_eldo_en {
		wcd_eldo {
			pins = "gpio7";
			function = "gpio";
			output-low;
			bias-disable;
			power-source = <0>; /* VPH_PWR */
		};
	};
};
